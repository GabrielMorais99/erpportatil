{
    "version": 2,
    "builds": [
        {
            "src": "api/**/*.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/css/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/js/(.*)",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*\\.(css|js|json|png|jpg|ico|svg|woff|woff2))",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}
